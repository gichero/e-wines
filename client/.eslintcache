/gichero/React/e-wine/client/src/reducers/userReducer.js",[],"/Users/gichero/React/e-wine/client/src/pages/CartPage.js",[],"/Users/gichero/React/e-wine/client/src/pages/ProductPage.js",[],"/Users/gichero/React/e-wine/client/src/pages/UserLoginPage.js",[],"/Users/gichero/React/e-wine/client/src/actions/userActions.js",[],"/Users/gichero/React/e-wine/client/src/components/FormContainer/FormContainer.js",[],"/Users/gichero/React/e-wine/client/src/pages/UserRegisterPage.js",[],"/Users/gichero/React/e-wine/client/src/pages/UserProfilePage.js",[],"/Users/gichero/React/e-wine/client/src/pages/ShippingPage.js",[],"/Users/gichero/React/e-wine/client/src/components/CheckoutSteps/CheckoutSteps.js",[],"/Users/gichero/React/e-wine/client/src/pages/PaymentPage.js",[],"/Users/gichero/React/e-wine/client/src/pages/PlaceOrderPage.js",["163"],"/** @format */\n\nimport React, { useEffect } from \"react\";\nimport { Link } from \"react-router-dom\";\nimport { Button, Row, Col, ListGroup, Image, Card } from \"react-bootstrap\";\nimport { useSelector, useDispatch } from \"react-redux\";\nimport Message from \"../components/Message/Message\";\nimport CheckoutSteps from \"../components/CheckoutSteps/CheckoutSteps\";\nimport { createOrder } from \"../actions/orderActions\";\n\nconst PlaceOrderPage = ({ history }) => {\n\tconst dispatch = useDispatch();\n\n\tconst cart = useSelector((state) => state.cart);\n\n\t//Calculate prices\n\tconst addDecimals = (num) => {\n\t\treturn (Math.round(num * 100) / 100).toFixed(2);\n\t};\n\n\tcart.itemsPrice = addDecimals(\n\t\tcart.cartItems.reduce((acc, item) => acc + item.price * item.qty, 0)\n\t);\n\n\tcart.shippingPrice = addDecimals(cart.itemsPrice > 100 ? 0 : 15);\n\n\tcart.taxPrice = addDecimals(Number(0.05 * cart.itemsPrice).toFixed(2));\n\n\tcart.totalPrice = (\n\t\tNumber(cart.itemsPrice) +\n\t\tNumber(cart.shippingPrice) +\n\t\tNumber(cart.taxPrice)\n\t).toFixed(2);\n\n\tconst orderCreate = useSelector((state) => state.orderCreate);\n\tconst { order, success, error } = orderCreate;\n\n\tuseEffect(() => {\n\t\tif (success) {\n\t\t\thistory.push(`/order/${order._id}`);\n\t\t}\n\t}, [history, success]);\n\n\tconst placeOrderHandler = () => {\n\t\tdispatch(\n\t\t\tcreateOrder({\n\t\t\t\torderItems: cart.cartItems,\n\t\t\t\tshippingAddress: cart.shippingAddress,\n\t\t\t\tpaymentMethod: cart.paymentMethod,\n\t\t\t\titemsPrice: cart.itemsPrice,\n\t\t\t\tshippingPrice: cart.shippingPrice,\n\t\t\t\ttaxPrice: cart.taxPrice,\n\t\t\t\ttotalPrice: cart.totalPrice,\n\t\t\t})\n\t\t);\n\t};\n\n\treturn (\n\t\t<>\n\t\t\t<CheckoutSteps step1 step2 step3 step4 />\n\n\t\t\t<Row>\n\t\t\t\t<Col md={8}>\n\t\t\t\t\t<ListGroup variant=\"flush\">\n\t\t\t\t\t\t<ListGroup.Item>\n\t\t\t\t\t\t\t<h1>Shipping</h1>\n\t\t\t\t\t\t\t<h5>\n\t\t\t\t\t\t\t\t<strong>Address:</strong> {cart.shippingAddress.address},{\" \"}\n\t\t\t\t\t\t\t\t{cart.shippingAddress.city} {cart.shippingAddress.postalCode},{\" \"}\n\t\t\t\t\t\t\t\t{cart.shippingAddress.country}\n\t\t\t\t\t\t\t</h5>\n\t\t\t\t\t\t</ListGroup.Item>\n\n\t\t\t\t\t\t<ListGroup.Item>\n\t\t\t\t\t\t\t<h2>Payment Method</h2>\n\t\t\t\t\t\t\t<strong>Method: </strong>\n\t\t\t\t\t\t\t{cart.paymentMethod}\n\t\t\t\t\t\t</ListGroup.Item>\n\n\t\t\t\t\t\t<ListGroup.Item>\n\t\t\t\t\t\t\t<h2>Order Items</h2>\n\t\t\t\t\t\t\t{cart.cartItems.length === 0 ? (\n\t\t\t\t\t\t\t\t<Message>Empty Cart</Message>\n\t\t\t\t\t\t\t) : (\n\t\t\t\t\t\t\t\t<ListGroup variant=\"flush\">\n\t\t\t\t\t\t\t\t\t{cart.cartItems.map((item, index) => (\n\t\t\t\t\t\t\t\t\t\t<ListGroup.Item key={index}>\n\t\t\t\t\t\t\t\t\t\t\t<Row>\n\t\t\t\t\t\t\t\t\t\t\t\t<Col md={1}>\n\t\t\t\t\t\t\t\t\t\t\t\t\t<Image\n\t\t\t\t\t\t\t\t\t\t\t\t\t\tsrc={item.image}\n\t\t\t\t\t\t\t\t\t\t\t\t\t\talt={item.name}\n\t\t\t\t\t\t\t\t\t\t\t\t\t\tfluid\n\t\t\t\t\t\t\t\t\t\t\t\t\t\trounded\n\t\t\t\t\t\t\t\t\t\t\t\t\t/>\n\t\t\t\t\t\t\t\t\t\t\t\t</Col>\n\t\t\t\t\t\t\t\t\t\t\t\t<Col>\n\t\t\t\t\t\t\t\t\t\t\t\t\t<Link to={`/product/${item.product}`}>\n\t\t\t\t\t\t\t\t